<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppBaseTheme" parent="@android:style/Theme.Holo.Light.DarkActionBar" />
    <style name="Platform.AppCompat" parent="@style/Platform.V14.AppCompat" />
    <style name="Platform.AppCompat.Light" parent="@style/Platform.V14.AppCompat.Light" />
    <style name="com_ad4screen_sdk_popup_inapp_other_button_style" parent="@android:style/Widget.DeviceDefault.Button.Borderless.Small" />
    <style name="com_ad4screen_sdk_popup_inapp_primary_button_style" parent="@android:style/Widget.DeviceDefault.Button.Borderless.Small" />
    <style name="com_ad4screen_sdk_popup_inapp_secondary_button_style" parent="@android:style/Widget.DeviceDefault.Button.Borderless.Small" />
    <style name="com_ad4screen_sdk_popup_notif_other_button_style" parent="@android:style/Widget.DeviceDefault.Button.Borderless.Small" />
    <style name="com_ad4screen_sdk_popup_notif_primary_button_style" parent="@android:style/Widget.DeviceDefault.Button.Borderless.Small" />
    <style name="com_ad4screen_sdk_popup_notif_secondary_button_style" parent="@android:style/Widget.DeviceDefault.Button.Borderless.Small" />
    <style name="com_ad4screen_sdk_theme_interstitial" parent="@android:style/Theme.DeviceDefault">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowCloseOnTouchOutside">false</item>
    </style>
    <style name="com_ad4screen_sdk_theme_popup" parent="@android:style/Theme.DeviceDefault.Dialog">
        <item name="android:windowCloseOnTouchOutside">false</item>
        <item name="com_ad4screen_sdk_popup_inapp_primary_button">@style/com_ad4screen_sdk_popup_inapp_primary_button_style</item>
        <item name="com_ad4screen_sdk_popup_inapp_secondary_button">@style/com_ad4screen_sdk_popup_inapp_secondary_button_style</item>
        <item name="com_ad4screen_sdk_popup_inapp_other_button">@style/com_ad4screen_sdk_popup_inapp_other_button_style</item>
        <item name="com_ad4screen_sdk_popup_inapp_title_layout">@null</item>
        <item name="com_ad4screen_sdk_popup_notif_title_layout">@null</item>
        <item name="com_ad4screen_sdk_popup_inapp_layout">@layout/com_ad4screen_sdk_popup_inapp</item>
        <item name="com_ad4screen_sdk_popup_notif_layout">@layout/com_ad4screen_sdk_popup_notif</item>
        <item name="com_ad4screen_sdk_popup_inapp_buttons_layout">@layout/com_ad4screen_sdk_popup_inapp_buttons</item>
        <item name="com_ad4screen_sdk_popup_notif_buttons_layout">@layout/com_ad4screen_sdk_popup_notif_buttons</item>
        <item name="com_ad4screen_sdk_popup_notif_primary_button">@style/com_ad4screen_sdk_popup_inapp_primary_button_style</item>
        <item name="com_ad4screen_sdk_popup_notif_secondary_button">@style/com_ad4screen_sdk_popup_inapp_secondary_button_style</item>
        <item name="com_ad4screen_sdk_popup_notif_other_button">@style/com_ad4screen_sdk_popup_inapp_other_button_style</item>
    </style>
    <style name="Base.TextAppearance.AppCompat.Button" parent="@style/Base.TextAppearance.AppCompat">
        <item name="android:textSize">@dimen/abc_text_size_button_material</item>
        <item name="android:textColor">?android:textColorPrimary</item>
        <item name="android:textAllCaps">true</item>
    </style>
    <style name="TextAppearance.StatusBar.EventContent" parent="@android:style/TextAppearance.StatusBar.EventContent" />
    <style name="TextAppearance.StatusBar.EventContent.Info" parent="@style/TextAppearance.StatusBar.EventContent" />
    <style name="TextAppearance.StatusBar.EventContent.Line2" parent="@style/TextAppearance.StatusBar.EventContent">
        <item name="android:textSize">@dimen/notification_subtext_size</item>
    </style>
    <style name="TextAppearance.StatusBar.EventContent.Time" parent="@style/TextAppearance.StatusBar.EventContent" />
    <style name="TextAppearance.StatusBar.EventContent.Title" parent="@android:style/TextAppearance.StatusBar.EventContent.Title" />
    <style name="Platform.V14.AppCompat" parent="@style/Platform.V11.AppCompat">
        <item name="android:actionModeSelectAllDrawable">?actionModeSelectAllDrawable</item>
        <item name="android:listPreferredItemPaddingLeft">@dimen/abc_list_item_padding_horizontal_material</item>
        <item name="android:listPreferredItemPaddingRight">@dimen/abc_list_item_padding_horizontal_material</item>
    </style>
    <style name="Platform.V14.AppCompat.Light" parent="@style/Platform.V11.AppCompat.Light">
        <item name="android:actionModeSelectAllDrawable">?actionModeSelectAllDrawable</item>
        <item name="android:listPreferredItemPaddingLeft">@dimen/abc_list_item_padding_horizontal_material</item>
        <item name="android:listPreferredItemPaddingRight">@dimen/abc_list_item_padding_horizontal_material</item>
    </style>
</resources>
